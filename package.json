{
  "name": "awrapper",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "turbo": "turbo",
    "ci": "turbo run test"
  },
  "dependencies": {
    "@fastify/formbody": "^7.0.0",
    "better-sqlite3": "^11.10.0",
    "execa": "^9.3.0",
    "fastify": "^4.28.1",
    "fastify-type-provider-zod": "^2.0.0",
    "fs-extra": "^11.2.0",
    "nanoid": "^5.0.7",
    "pino": "^9.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.4",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.10.0",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "turbo": "^2.1.3",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
